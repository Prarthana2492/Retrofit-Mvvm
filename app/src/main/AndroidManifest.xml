<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.renewin.FarmPeFarmer">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permisssion.ACCESS_COARSE_LOCATION" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>



    <application
        android:allowBackup="true"
        android:icon="@drawable/farm_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/farm_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.renewin.FarmPeFarmer.Activity.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name="com.renewin.FarmPeFarmer.Activity.ForgotPasswordNew"
            android:screenOrientation="portrait"/>

        <activity android:name="com.renewin.FarmPeFarmer.Activity.SelectLanguageActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.renewin.FarmPeFarmer.Activity.EnterOTP"
            android:screenOrientation="portrait"/>
        <activity android:name="com.renewin.FarmPeFarmer.Activity.Thank_U_New"
            android:screenOrientation="portrait"/>

        <activity android:name="com.renewin.FarmPeFarmer.Activity.LandingPageActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            />

        <activity android:name="com.renewin.FarmPeFarmer.Activity.ResetPasswordNew"
            android:screenOrientation="portrait"/>
        <activity android:name="com.renewin.FarmPeFarmer.Activity.FarmerUser"
            android:screenOrientation="portrait"/>

        <activity android:name="com.renewin.FarmPeFarmer.Activity.FarmerActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.renewin.FarmPeFarmer.Activity.SignUpActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.renewin.FarmPeFarmer.Activity.FarmerActivityNext"
            android:screenOrientation="portrait"/>


    </application>


</manifest>